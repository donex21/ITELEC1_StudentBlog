<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studentlog">

    <application
        android:allowBackup="true"
        android:icon="@drawable/front"
        android:label="@string/app_name"
        android:roundIcon="@drawable/front"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UpdateStudentActivity"></activity>
        <activity android:name=".ViewStudentActivity" />
        <activity android:name=".AddStudent" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>